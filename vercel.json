{
  "version": 2,
  "name": "food-delivery",
  "public":true,
  "builds": [
     { 
       "src": "index.js", 
       "use": "@vercel/node" 
    },
     {
       "src" : "public/**",
       "use" : "@vercel/static"
    }
  ],
  "routes": [
     { 
       "src": "/(.*)", 
       "dest": "/index.js" ,
       "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
    "headers": {
      "Access-Control-Allow-Origin": "*"}
    },
     {
       "src": "/(.+\\.[a-z]+)$",
       "dest": "/public/$1"
     }
  ]
  
  
  
}